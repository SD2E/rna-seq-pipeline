{
  "name": "{{ agave.username }}-{{ app.name }}",
  "version": "{{ app.version }}",
  "executionType": "HPC",
  "executionSystem": "hpc-tacc-wrangler-{{ agave.username }}-custom",
  "parallelism": "SERIAL",
  "deploymentPath": "{{ agave.username }}/apps/{{ app.name }}-{{ app.version }}",
  "deploymentSystem": "data-sd2e-projects-users",
  "defaultProcessorsPerNode": 1,
  "defaultNodeCount": 1,
  "defaultQueue": "all",
  "label": "{{ date_time.iso8601_micro }}",
  "modules": ["load tacc-singularity"],
  "shortDescription": "{{ app.description }}",
  "templatePath": "runner-template.sh",
  "testPath": "tester.sh",
  "inputs": [
  {
  "id": "sample_paths",
  "value": {
    "default": "",
    "order": 0,
    "required": false,
    "visible": true
    }
  },
  {
  "id": "gff",
  "value": {
    "default": "",
    "order": 0,
    "required": false,
    "visible": true
    }
  }],
  "parameters": [
  {
    "id": "path_bam_dir",
    "value": {
      "default": null,
      "description": "path to the bam directory",
      "type": "string",
      "visible": true,
      "required": false
    }
  },
    {
      "id": "path_gff",
      "value": {
        "default": null,
        "required": false,
        "type": "string",
        "visible": true
      }
    },
  {
    "id": "CONTAINER_IMAGE",
    "value": {
      "default": "{{ docker.organization }}/{{ docker.repo }}:{{ docker.tag }}",
      "description": "Container Image. Do not edit.",
      "type": "string",
      "visible": false,
      "required": true
    }
  }
  ],
  "outputs": []
}
